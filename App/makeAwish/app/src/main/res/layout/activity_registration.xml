<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="good.force.teah.makeawish.Registration">


        <TextView
            android:id="@+id/textView3"
            android:layout_width="0dp"
            android:layout_height="23dp"
            android:text="Aadhar Number"
            android:textAlignment="textEnd"
            android:gravity="end"
            app:layout_constraintRight_toLeftOf="@+id/radio_pirates"
            tools:layout_constraintRight_creator="1"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="15dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/aadhar"
            tools:layout_constraintBaseline_creator="1"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="15dp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="0dp"
            android:layout_height="23dp"
            android:text="gender"
            android:textAlignment="textEnd"
            android:gravity="end"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="@+id/aadhar"
            app:layout_constraintRight_toRightOf="@+id/textView3"
            android:layout_marginTop="59dp"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="57dp"
            app:layout_constraintLeft_toLeftOf="@+id/textView4"
            app:layout_constraintTop_toTopOf="@+id/name" />

        <RadioGroup xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </RadioGroup>

        <TextView
            android:id="@+id/textView8"
            android:layout_width="0dp"
            android:layout_height="25dp"
            android:text="Date of Birth"
            android:textAlignment="textEnd"
            android:gravity="end"
            app:layout_constraintRight_toLeftOf="@+id/dateOfBirth"
            tools:layout_constraintRight_creator="1"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="14dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/dateOfBirth"
            tools:layout_constraintBaseline_creator="1"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="14dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="66dp"
            android:layout_height="23dp"
            android:text="Contact"
            android:textAlignment="textEnd"
            android:gravity="end"
            app:layout_constraintRight_toLeftOf="@+id/contact"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="@+id/Volunteer"
            android:layout_marginEnd="14dp"
            android:layout_marginTop="191dp"
            android:layout_marginBottom="191dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginRight="14dp" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="0dp"
            android:layout_height="25dp"
            android:text="Name"
            android:textAlignment="textEnd"
            android:gravity="end"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="@+id/name"
            app:layout_constraintRight_toRightOf="@+id/textView6"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/textView2"
            app:layout_constraintTop_toTopOf="@+id/name" />

        <EditText
            android:id="@+id/dateOfBirth"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="date"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/contact"
            android:layout_marginTop="17dp"
            app:layout_constraintTop_toBottomOf="@+id/contact"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/contact" />

        <EditText
            android:id="@+id/name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            android:layout_marginEnd="25dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="34dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginRight="25dp" />

        <EditText
            android:id="@+id/aadhar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="number"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/name"
            android:layout_marginTop="53dp"
            app:layout_constraintTop_toBottomOf="@+id/name"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/name" />

        <EditText
            android:id="@+id/contact"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="number"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/aadhar"
            android:layout_marginTop="18dp"
            app:layout_constraintTop_toBottomOf="@+id/aadhar"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/aadhar" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="75dp"
            android:layout_height="36dp"
            android:text="Type"
            android:textAlignment="textEnd"
            android:gravity="end"
            app:layout_constraintRight_toLeftOf="@+id/doctor"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="16dp"
            android:layout_marginTop="151dp"
            app:layout_constraintTop_toBottomOf="@+id/aadhar"
            android:layout_marginBottom="151dp"
            android:layout_marginRight="16dp" />

        <CheckBox
            android:id="@+id/doctor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Doctor"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/Volunteer"
            app:layout_constraintTop_toTopOf="@+id/textView6" />

        <CheckBox
            android:id="@+id/Volunteer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Volunteer"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/Donor"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="12dp"
            app:layout_constraintLeft_toLeftOf="@+id/Donor" />

        <CheckBox
            android:id="@+id/Donor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Donor"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/button"
            android:layout_marginEnd="21dp"
            app:layout_constraintRight_toRightOf="@+id/button"
            android:layout_marginBottom="7dp"
            android:layout_marginRight="21dp" />

        <Button
            android:id="@+id/button_submit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Submit"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="7dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <RadioButton
            android:id="@+id/radio_pirates"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="onRadioButtonClicked"
            android:text="Male"
            android:layout_marginStart="1dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/radio_ninjas"
            tools:layout_constraintBaseline_creator="1"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/name"
            android:layout_marginLeft="1dp" />

        <RadioButton
            android:id="@+id/radio_ninjas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="onRadioButtonClicked"
            android:text="Female"
            tools:layout_constraintTop_creator="1"
            android:layout_marginStart="6dp"
            android:layout_marginTop="7dp"
            app:layout_constraintTop_toBottomOf="@+id/name"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toRightOf="@+id/radio_pirates"
            android:layout_marginLeft="6dp" />


</android.support.constraint.ConstraintLayout>
